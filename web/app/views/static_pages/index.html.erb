<!-- preloader -->

<id id="preloader">
	<div id="status">&nbsp;</div>
</id>

<!-- Home -->
<section id="home">
	<!-- Background Video -->
	<%= video_tag '/video/solo.mp4', class: "home-bg-video", autoplay: true, loop: true do %>
		<source scr="/video/solo.jpg">
		<source scr="/video/solo.ogv">
		<source scr="/video/solo.webm">
	<% end %>

	<!-- Overlay -->
	<div id="home-overlay"></div>

	<!-- Home Content -->
	<div id="home-content">
		<div id="home-content-inner" class="text-center">
			<div id="home-heading">
				<h1 id="home-heading1">Digital</h1>
				<br>
				<h1 class="home-heading-2">Creative<span>Agency</span></h1>
			</div>

			<div id="home-text">
				<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. </p>
			</div>

			<div id="home-btn">
				<a href="#about" class="btn btn-general btn-home">Start Now</a>
			</div>
		</div>
	</div>

	<!-- Arrow Down -->
	<div href="#about" id="arrow-down">
		<i class="fa fa-angle-down"></i>
	</div>
</section>

<!-- About -->
<section id="about">
	<div id="about-01">
		<div class="content-box-lg">
			<div class="container">
				<div class="row">
					<div class="col-md-6">
						<div id="about-left">
							<div class="vertical-heading">
								<h5>Who We Are</h5>
								<h2>A <strong>Story</strong><br>About Us</h2>
							</div>
						</div>
					</div>
					<div class="col-md-6">
						<div id="about-right">
							<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Tempora nihil officiis quam blanditiis esse qui aperiam vitae doloribus deleniti inventore.</p>
